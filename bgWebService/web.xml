<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
  <display-name>BioGeomancer Web Services</display-name>
   <description>cache</description>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

  <servlet>
      <servlet-name>SingleGeoreferenceWebService</servlet-name>
      <servlet-class>edu.berkeley.biogeomancer.webservice.SingleGeoreferenceWebService</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>SingleGeoreferenceWebService</servlet-name>
      <url-pattern>/single</url-pattern>
  </servlet-mapping>

  <servlet>
      <servlet-name>BatchGeoreferenceWebService</servlet-name>
      <servlet-class>edu.berkeley.biogeomancer.webservice.BatchGeoreferenceWebService</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>BatchGeoreferenceWebService</servlet-name>
      <url-pattern>/batch</url-pattern>
  </servlet-mapping>
  	
</web-app>
